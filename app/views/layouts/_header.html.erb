    <div class="container">
      <div class="row-fluid box">
        <div class="span12"> 
          <div class="span4">
            <h2><%= link_to "Xemoff.com",'#', id: "logo" %></h2>
          </div>
          <div class="span2"%></div>
          <div class="span4">
            <nav>
              <ul class="nav pull-right diplay">
                <li class="jumper"><h5><%= link_to("Contact", 'javascript:;',:id=>"contact")%></h5></li>
              </ul>	
            </nav>
          </div>  
    </div> 
  </div>
</header>
<div class="show_contact" style="display:none;">
  <%= render :partial => "users/form" %>
</div> 

<script>
  $(window).scroll(function (event) {
    var top = $('.box').offset().top;
    var y = $(this).scrollTop();
    if (y >= 730 ) { $('.box').addClass('fixed');}
    else { $('.box').removeClass('fixed');}
    $("#contact").click(function(){
      $(".show_contact").toggle();
    });
  });
</script>
